<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(add_page) -%>">
  <span class='title'>
    <%= add_page.name %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.add_pages_add_page_path(add_page),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_add_pages_admin_add_page_path(add_page),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.add_pages_admin_add_page_path(add_page),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => add_page.name),
        :method => :delete %>
  </span>
</li>
